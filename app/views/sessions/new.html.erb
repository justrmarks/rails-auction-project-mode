<div class="twelve wide centered column">
  <%= form_with url: sessions_path, class: "ui form" do |f| %>

    <div class="four wide required field">
      <%= f.label :username %>
      <%= f.text_field :username %>
    </div>

    <div class="four wide required field">
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <%= f.submit "Log in", class: "ui teal button"%>
  
  <% end %>
</div>
